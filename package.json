{
  "name": "inputotp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "14.1.4",
    "react": "18.2.0",
    "react-dom": "18.2"
  },
  "devDependencies": {
    "@types/node": "20.8.10",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.0.1",
    "eslint": "8.5.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "7.0.4",
    "lint-staged": "12.1.3",
    "postcss": "8.4.31",
    "prettier": "2.8.8",
    "tailwindcss": "3.4.3",
    "typescript": "5.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,html,css}": [
      "prettier --write .",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
